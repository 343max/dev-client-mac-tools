{
  "name": "dev-client-mac-tools-example",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "native-dev:project:ios": "rm -rf ios/ && expo prebuild --platform ios",
    "native-dev:project:android": "rm -rf android/ && expo prebuild --platform android"
  },
  "dependencies": {
    "expo": "^49.0.2",
    "expo-dev-menu": "^3.1.7",
    "expo-dev-menu-interface": "^1.3.0",
    "expo-manifests": "^0.7.1",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
